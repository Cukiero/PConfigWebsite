@model PConfigAuth.Models.TempConfigViewModel;



<div class="row">
    <div class="col-md-12" id="tempconfig-column">
        <div class="panel-default">
            <div class="panel panel-heading"><a asp-controller="Configure" asp-action="AddConfig" asp-route-hwtype="@ViewData["HwType"]" class="btn btn-primary" id="add-conf-bt"><span class="glyphicon glyphicon-save"></span></a>Temporary Configuration @Html.Raw(ViewData["ServerMessage"])</div>
