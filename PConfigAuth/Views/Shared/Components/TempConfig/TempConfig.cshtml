                                <div class="well temp-conf-well-2">
                                    <div class="row temp-conf-category-row">
                                        <span class="temp-conf-category">Storage - @(i+1)</span>
                                        <a asp-controller="Configure" asp-action="RemoveStorage" asp-route-id="@Model.storages[i].SessionKeyNumber" asp-route-hwtype="@ViewData["HwType"]" class="temp-conf-remove-bt"><span class="glyphicon glyphicon-remove"></span></a>
                                    </div>
                                    <div class="row">
                                        <table class="table temp-conf-table">
