                                    <table class="table temp-conf-table">
                                        <tbody>
                                            <tr>
                                                <td><span class="temp-conf-names">@Html.DisplayNameFor(Model => Model.cpu.Name):</span></td>
                                                <td><span class="temp-conf-values">@Html.DisplayFor(Model => Model.cpu.Name)</span></td>
                                            </tr>
                                            <tr>
                                                <td><span class="temp-conf-names">@Html.DisplayNameFor(Model => Model.cpu.Manufacturer):</span></td>
                                                <td><span class="temp-conf-values">@Html.DisplayFor(Model => Model.cpu.Manufacturer)</span></td>
                                            </tr>
                                            <tr>
                                                <td><span class="temp-conf-names">@Html.DisplayNameFor(Model => Model.cpu.Series):</span></td>
